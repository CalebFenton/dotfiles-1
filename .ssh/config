Include config.d/*

ServerAliveInterval 120
AddKeysToAgent yes

Host *grundy.io
  IdentityFile ~/.ssh/id_rsa.personal
  User rich

Host prometheus*
  IdentityFile ~/.ssh/id_rsa.personal
  User rich

Host *github.com
  IdentityFile ~/.ssh/id_rsa.personal
  IdentityFile ~/.ssh/id_rsa
  User git

Host *bitbucket.org
  IdentityFile ~/.ssh/id_rsa.personal
  IdentityFile ~/.ssh/id_rsa
  User git

Host mars.stinkyink.com
  HostName ra.stinkyink.com
  User rich
  IdentityFile ~/.ssh/id_rsa.work
  ForwardAgent yes
  Port 23

Host venus.stinkyink.com
  HostName ra.stinkyink.com
  User rich
  IdentityFile ~/.ssh/id_rsa.work
  ForwardAgent yes
  Port 24

Host work.stinkyink.com
  HostName praxis
  User rich
  ProxyCommand ssh -W %h:%p mars.stinkyink.com
  ForwardAgent yes
  IdentityFile ~/.ssh/id_rsa.work

Host gateway.stinkyink.com
  User deploy
  ForwardAgent yes
  IdentityFile ~/.ssh/id_rsa.work
  ProxyCommand ssh -W %h:%p rich@mars.stinkyink.com
  ProxyCommand none

Host !mars.stinkyink.com !venus.stinkyink.com !work.stinkyink.com !gateway.stinkyink.com !ra.stinkyink.com *.stinkyink.com
  User deploy
  IdentityFile ~/.ssh/id_rsa.work
  ProxyCommand ssh -W %h:%p deploy@gateway.stinkyink.com
  ForwardAgent yes

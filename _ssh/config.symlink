ServerAliveInterval 120

Host *richgrundy.com
  IdentityFile ~/.ssh/id_rsa.personal
  User rich

Host *github.com
  IdentityFile ~/.ssh/id_rsa.personal
  User git

Host *bitbucket.org
  IdentityFile ~/.ssh/id_rsa.personal
  User git

Host gateway.stinkyink.com
  User deploy
  IdentityFile ~/.ssh/id_rsa.work
  ProxyCommand none
  ForwardAgent yes

Host *.stinkyink.com
  User deploy
  IdentityFile ~/.ssh/id_rsa.work
  ProxyCommand ssh -W %h:%p deploy@gateway.stinkyink.com
  ForwardAgent yes
